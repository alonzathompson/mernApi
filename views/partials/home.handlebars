<div class="container-fluid py-2">
  <div class="row">
    <div class="col-md-12 text-center" >
      <h1>Welcome {{email}}</h1>
    </div>
    <div class="col-md-6">
      <h1>Whats New</h1>
    </div>
  </div>
  <div class="container">
    <button type="button" name="button" id="goToProducts" class="btn btn-primary">Catalogue</button>
  </div>
  <div class="">
    {{>products}}
  </div>
  <script type="text/javascript">

    const getProductBn = document.getElementById('goToProducts');
    const goToProd = () => {
      const getToken = window.localStorage.getItem('token');
      console.log(getToken)
      fetch('./products',{
        method: 'GET',
        mode: 'cors',
        redirected: "follow",
        headers: new Headers({
          Authorization: `${getToken}`
        })
      })
        .then((response)=>{
          console.log(getToken);
          console.log(response);

        })
    }

    getProductBn.addEventListener('click', goToProd)
  </script>
</div>
